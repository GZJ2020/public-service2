<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="DB2Tables" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="org.gjt.mm.mysql.Driver"
			connectionURL="jdbc:mysql://localhost:3306/fund?useUnicode=true&amp;characterEncoding=utf-8"
			userId="root" password="root">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="cn.sz.gz.pojo"
			targetProject="src">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="/" targetProject="src">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>

		<table tableName="admin"></table>
		<table tableName="bank_card"></table>
		<table tableName="fixed_investment"></table>
		<table tableName="fund"></table>
		<table tableName="fund_order"></table>
		<table tableName="shopping_cart"></table>
		<table tableName="ums_ralation"></table>
		<table tableName="ums_resource"></table>
		<table tableName="ums_role"></table>
		<table tableName="ums_role_resource_relation"></table>
		<table tableName="user"></table>
	</context>

</generatorConfiguration>